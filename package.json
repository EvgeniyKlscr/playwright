{
  "name": "playwright",
  "version": "1.0.0",
  "main": "index.js",
  "lint-staged": {
    "packages/**/src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "packages/**/src/**/*.{ts,tsx,js,jsx,json,md,yml,yaml,css,scss}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:debug": "npx playwright test --debug",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --config .prettierrc src/**/*.ts --check src",
    "prettier:fix": "prettier --config .prettierrc src/**/*.ts --write src",
    "format": "npm run lint && npm run prettier",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.55.1",
    "@types/ajv": "^0.0.5",
    "@types/dotenv": "^6.1.1",
    "@types/moment": "^2.11.29",
    "@types/node": "^24.6.2",
    "@types/numeral": "^2.0.5",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/bson": "^4.0.5",
    "@types/lodash": "^4.17.20",
    "ajv": "^8.17.1",
    "bson": "^7.0.0",
    "dotenv": "^17.2.3",
    "lint-staged": "^16.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "numeral": "^2.0.6"
  }
}
